# Zsh Aliases - CLI tools via npx
# Source in .zshrc: [ -f ~/.zsh_aliases ] && source ~/.zsh_aliases
#
# Format: alias <cmd>="npx -y [--package=<pkg>] <cmd>"
# Use --package only when package name differs from executable

alias agent-browser="npx -y agent-browser"
alias analyze-trace="npx -y --package=@typescript/analyze-trace analyze-trace"
alias asana="npx -y --package=asana-cli asana"
alias ccline="npx -y --package=@cometix/ccline ccline"
alias codex="npx -y --package=@openai/codex codex"
alias copilot="npx -y --package=@github/copilot copilot"
alias cz="npx -y --package=commitizen cz"
alias dotenvx="npx -y --package=@dotenvx/dotenvx dotenvx"
alias firebase="npx -y --package=firebase-tools firebase"
alias g2d="npx -y --package=@g2d/cli g2d"
alias gemini="npx -y --package=@google/gemini-cli gemini"
alias http-server="npx -y http-server"
alias is="npx -y --package=@microsoft/inshellisense is"
alias na="npx -y --package=@antfu/ni na"
alias nci="npx -y --package=@antfu/ni nci"
alias ngrok="npx -y ngrok"
alias ni="npx -y --package=@antfu/ni ni"
alias nip="npx -y --package=@antfu/nip nip"
alias nlx="npx -y --package=@antfu/ni nlx"
alias nr="npx -y --package=@antfu/ni nr"
alias nun="npx -y --package=@antfu/ni nun"
alias nup="npx -y --package=@antfu/ni nup"
alias qnm="npx -y qnm"
alias serve="npx -y serve"
alias skills="npx -y skills"
alias taze="npx -y taze"
alias tmole="npx -y --package=tunnelmole tmole"
alias vercel="npx -y vercel"
alias wrangler="npx -y wrangler"
